    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) 附加",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/drv_lib_proj.elf",
            "cwd": "${workspaceFolder}",
            "miDebuggerServerAddress": "localhost:2331",
            "MIMode": "gdb",
            "miDebuggerPath": "E:/Program Files (x86)/GNU Arm Embedded Toolchain/10 2020-q4-major/bin/arm-none-eabi-gdb.exe",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}